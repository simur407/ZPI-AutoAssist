<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.edu.pwr.zpi.autoasystent">

    <application
        android:name=".AutoAsystentApplication"
        android:allowBackup="true"
        android:icon="@drawable/pancake_bunny"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.CarAddActivity" />
        <activity android:name=".view.activity.CarActivity" />
        <activity android:name=".view.activity.RefuelViewActivity" />
        <activity android:name=".view.activity.ServiceJobsActivity" />
        <activity android:name=".view.activity.RefuelingAddActivity"/>
        <activity android:name=".view.activity.ServiceActivity"/>
        <activity android:name=".view.activity.ReportFuelActivity" />
        <activity android:name=".view.activity.ReportServicesActivity" />
        <activity android:name=".view.activity.ReportSummaryActivity" />


        <meta-data
            android:name="DATABASE"
            android:value="auto_asystent.db"/>
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="pl.edu.pwr.zpi" />

    </application>

</manifest>